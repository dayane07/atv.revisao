# Peça ao usuário para digitar uma frase
frase = input("Digite uma frase: ")

# Converta a frase para minúsculas para facilitar a contagem
frase = frase.lower()

# Inicialize os contadores
vogais = 0
consoantes = 0

# Defina as vogais
lista_vogais = "aeiou"

# Percorra a frase, caractere por caractere
for char in frase:
    # Verifique se o caractere é uma letra
    if 'a' <= char <= 'z':
        # Se for uma vogal, incremente o contador de vogais
        if char in lista_vogais:
            vogais += 1
        # Caso contrário, é uma consoante, então incremente o contador de consoantes
        else:
            consoantes += 1

# Exiba o resultado
print(f"Número de vogais: {vogais}")
print(f"Número de consoantes: {consoantes}")